               :class => 'fa fa-trash-o' %> <%= link_to 'Edit Note', edit_drawing_note_path(@note.drawing, @note), :class => 'fa fa-pencil-square-o' %><% end %>
</div>
<div class="detail-page">
<p><%= @note.body %></p>
</div>
